{
  "name": "@miniurl/express-plugin",
  "type": "module",
  "version": "0.0.1",
  "description": "Self Hosted Url Minification",
  "main": "./dist/index.cjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julienvanbeveren/miniurl.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "release": {
    "branches": [
      "main",
      "next"
    ]
  },
  "scripts": {
    "dev": "nodemon",
    "build": "swc ./src -d build",
    "build:watch": "swc ./src -d build -w",
    "type-check": "tsc --noEmit",
    "bundle": "unbuild",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "format:check": "biome format ./src",
    "format:fix": "biome format ./src --write",
    "publish": "npm run type-check && npm run build && npm run bundle && npm publish"
  },
  "author": "Julien Van Beveren",
  "license": "ISC",
  "unbuild": {
    "entries": [
      "./src/index"
    ],
    "declaration": true
  },
  "peerDependencies": {
    "express": "4.x"
  },
  "dependencies": {
    "@miniurl/core": "^0.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@swc/cli": "^0.3.5",
    "@swc/core": "^1.3.107",
    "@swc/jest": "^0.2.31",
    "@types/express": "^4.x",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  }
}
